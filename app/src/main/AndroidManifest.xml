        android:theme="@style/AppTheme">
        <activity android:name=".SeleccionarFaseActivity"
            android:parentActivityName=".AdminActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".AdminActivity" />
        </activity>
        <activity
            android:name=".EngadirFaseActivity"
            android:parentActivityName=".AdminActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".AdminActivity"/>
        </activity>
        <activity
            android:name=".PontevedraActivity"
            android:parentActivityName=".ActivityUsuario">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".OurenseActivity"
